{
  "name": "eurosport",
  "version": "1.0.0",
  "description": "An example React, GraphQL, Next and Apollo",
  "main": "index.js",
  "scripts": {
    "dev": "next -p 7777",
    "build": "next build",
    "start": "next start",
    "test": "NODE_ENV=test jest --watch",
    "test-win": "SET NODE_ENV=test&& jest --watch",
    "heroku-postbuild": "next build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@zeit/next-typescript": "^1.1.1",
    "apollo-boost": "^0.3.1",
    "apollo-client": "^2.5.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.10.0",
    "date-fns": "^2.0.0-alpha.7",
    "downshift": "^3.2.10",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "lodash.debounce": "^4.0.8",
    "next": "^8.1.0",
    "next-with-apollo": "^3.4.0",
    "nprogress": "^0.2.0",
    "react": "^16.8.6",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-stripe-checkout": "^2.6.3",
    "react-transition-group": "^2.5.0",
    "styled-components": "^4.2.0",
    "waait": "^1.0.2"
  },
  "devDependencies": {
    "@types/next": "^8.0.5",
    "@types/nprogress": "^0.0.29",
    "@types/styled-components": "^4.1.14",
    "babel-plugin-module-resolver": "^3.2.0",
    "casual": "^1.6.0",
    "graphql-tools": "^4.0.4"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel",
          "@zeit/next-typescript/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel",
          "@zeit/next-typescript/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
